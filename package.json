	"dependencies": {
		"@astrojs/prefetch": "^0.4.0",
		"@astrojs/react": "^3.0.2",
		"@astrojs/tailwind": "^5.0.0",
		"@types/react": "^18.2.22",
		"@types/react-dom": "^18.2.7",
		"astro": "3.1.1",
		"astro-critters": "^2.0.11",
		"astro-purgecss": "^3.2.0",
		"exifreader": "^4.13.0",
		"farmhash": "^3.3.0",
		"fecha": "^4.2.3",
		"framer-motion": "^10.16.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-masonry-css": "^1.0.16",
		"sharp": "^0.32.6",
		"sqids": "^0.3.0",
		"tailwindcss": "^3.3.3"
	},
	"repository": "git@github.com:obviyus/musee.git",
	"devDependencies": {
		"@biomejs/biome": "^1.2.2",
